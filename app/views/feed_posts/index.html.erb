<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
  <tr>
    <th>Photo</th>
    <th>Description</th>
    <th colspan="4"></th>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td><%= image_tag post.image_url %></td>
      <td><%= post.content %></td>
      <td><%= link_to 'Show', user_post_path(post.user_id, post) %></td>
      <td><img src="/placeholders/like.png">(<%= post.likes.count %>)</td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

